{
  "__inputs": [],
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": { "type": "grafana", "uid": "-- Grafana --" },
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "description": "Production overview for the Autonomous Coding Agents (ACA) platform. Covers task throughput, latency distributions, LLM spend, error budget, and agent capacity.",
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "panels": [
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "id": 100,
      "title": "System Health Overview",
      "type": "row"
    },
    {
      "id": 1,
      "title": "Task Success Rate",
      "description": "Ratio of completed tasks to total finished tasks (completed + failed) over a 5-minute window. Green >= 95%, yellow >= 90%, red below 90%.",
      "type": "stat",
      "gridPos": { "h": 6, "w": 4, "x": 0, "y": 1 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "clamp_min(\n  rate(aca_task_completed_total[5m])\n  / (\n    rate(aca_task_completed_total[5m])\n    + rate(aca_task_failed_total[5m])\n  ),\n0)",
          "legendFormat": "Success Rate",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "decimals": 2,
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "#EAB839", "value": 0.90 },
              { "color": "green", "value": 0.95 }
            ]
          },
          "mappings": [
            {
              "type": "special",
              "options": { "match": "nan", "result": { "text": "No data", "color": "text" } }
            }
          ]
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "auto",
        "textMode": "auto",
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "auto"
      }
    },
    {
      "id": 2,
      "title": "Active Agents",
      "description": "Current number of agents actively processing tasks.",
      "type": "stat",
      "gridPos": { "h": 6, "w": 4, "x": 4, "y": 1 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "aca_agents_active",
          "legendFormat": "Active",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "blue", "value": null },
              { "color": "green", "value": 1 },
              { "color": "#EAB839", "value": 8 },
              { "color": "red", "value": 15 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "auto",
        "textMode": "auto",
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "auto"
      }
    },
    {
      "id": 3,
      "title": "Error Rate (per second)",
      "description": "Rate of errors per second over a 5-minute window. Alert fires at > 10 errors/min.",
      "type": "stat",
      "gridPos": { "h": 6, "w": 4, "x": 8, "y": 1 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "sum(rate(aca_errors_total[5m]))",
          "legendFormat": "Errors/s",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "decimals": 2,
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "#EAB839", "value": 0.05 },
              { "color": "red", "value": 0.17 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "auto",
        "textMode": "auto",
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "auto"
      }
    },
    {
      "id": 4,
      "title": "LLM Cost/Hour",
      "description": "Estimated hourly LLM spend extrapolated from the 5-minute rate of aca_llm_cost_total.",
      "type": "stat",
      "gridPos": { "h": 6, "w": 4, "x": 12, "y": 1 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "sum(rate(aca_llm_cost_total[5m])) * 3600",
          "legendFormat": "$/hr",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "currencyUSD",
          "decimals": 2,
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "#EAB839", "value": 5 },
              { "color": "red", "value": 10 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "auto",
        "textMode": "auto",
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "auto"
      }
    },
    {
      "id": 5,
      "title": "Tasks Completed (total)",
      "description": "Cumulative count of successfully completed tasks.",
      "type": "stat",
      "gridPos": { "h": 6, "w": 4, "x": 16, "y": 1 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "aca_task_completed_total",
          "legendFormat": "Completed",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "blue", "value": null }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "auto",
        "textMode": "auto",
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "auto"
      }
    },
    {
      "id": 6,
      "title": "Trace Throughput",
      "description": "Rate of exported OpenTelemetry spans per second.",
      "type": "stat",
      "gridPos": { "h": 6, "w": 4, "x": 20, "y": 1 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "rate(aca_spans_exported_total[5m])",
          "legendFormat": "spans/s",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "decimals": 1,
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "blue", "value": null }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "auto",
        "textMode": "auto",
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "auto"
      }
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 7 },
      "id": 101,
      "title": "Task Completion Rate",
      "type": "row"
    },
    {
      "id": 10,
      "title": "Task Completion Rate (tasks/min)",
      "description": "Per-minute rate of completed and failed tasks plotted over time.",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "rate(aca_task_completed_total[5m]) * 60",
          "legendFormat": "Completed/min",
          "refId": "A"
        },
        {
          "expr": "rate(aca_task_failed_total[5m]) * 60",
          "legendFormat": "Failed/min",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "opm",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 15,
            "lineWidth": 2,
            "pointSize": 5,
            "showPoints": "auto",
            "spanNulls": false,
            "stacking": { "mode": "none" },
            "axisPlacement": "auto",
            "thresholdsStyle": { "mode": "off" }
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "Failed/min" },
            "properties": [
              { "id": "color", "value": { "mode": "fixed", "fixedColor": "red" } }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "Completed/min" },
            "properties": [
              { "id": "color", "value": { "mode": "fixed", "fixedColor": "green" } }
            ]
          }
        ]
      },
      "options": {
        "tooltip": { "mode": "multi", "sort": "desc" },
        "legend": { "displayMode": "list", "placement": "bottom", "calcs": ["mean", "max"] }
      }
    },
    {
      "id": 11,
      "title": "Success Rate Over Time",
      "description": "Task success ratio plotted over time with threshold reference lines at 95% and 90%.",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "clamp_min(\n  rate(aca_task_completed_total[5m])\n  / (\n    rate(aca_task_completed_total[5m])\n    + rate(aca_task_failed_total[5m])\n  ),\n0)",
          "legendFormat": "Success Rate",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "min": 0,
          "max": 1,
          "decimals": 2,
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10,
            "lineWidth": 2,
            "pointSize": 5,
            "showPoints": "auto",
            "spanNulls": false,
            "thresholdsStyle": { "mode": "line" }
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "#EAB839", "value": 0.90 },
              { "color": "green", "value": 0.95 }
            ]
          },
          "color": { "mode": "thresholds" }
        },
        "overrides": []
      },
      "options": {
        "tooltip": { "mode": "single", "sort": "none" },
        "legend": { "displayMode": "list", "placement": "bottom", "calcs": ["mean", "min"] }
      }
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 16 },
      "id": 102,
      "title": "Task Duration (Histogram)",
      "type": "row"
    },
    {
      "id": 20,
      "title": "Task Duration p50 / p95 / p99",
      "description": "Percentile latencies computed from the aca_task_duration_seconds histogram. Use this to detect latency regressions and tail-latency issues.",
      "type": "timeseries",
      "gridPos": { "h": 9, "w": 12, "x": 0, "y": 17 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(aca_task_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "p50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(aca_task_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "p95",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(aca_task_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "p99",
          "refId": "C"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "decimals": 2,
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 8,
            "lineWidth": 2,
            "pointSize": 5,
            "showPoints": "auto",
            "spanNulls": false,
            "stacking": { "mode": "none" },
            "thresholdsStyle": { "mode": "dashed" }
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "transparent", "value": null },
              { "color": "red", "value": 120 }
            ]
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "p50" },
            "properties": [
              { "id": "color", "value": { "mode": "fixed", "fixedColor": "green" } }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "p95" },
            "properties": [
              { "id": "color", "value": { "mode": "fixed", "fixedColor": "orange" } }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "p99" },
            "properties": [
              { "id": "color", "value": { "mode": "fixed", "fixedColor": "red" } }
            ]
          }
        ]
      },
      "options": {
        "tooltip": { "mode": "multi", "sort": "desc" },
        "legend": { "displayMode": "table", "placement": "right", "calcs": ["mean", "max", "lastNotNull"] }
      }
    },
    {
      "id": 21,
      "title": "Task Duration Heatmap",
      "description": "Heatmap of task durations bucketed by the histogram. Darker cells indicate higher request volume at that latency.",
      "type": "heatmap",
      "gridPos": { "h": 9, "w": 12, "x": 12, "y": 17 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "sum(increase(aca_task_duration_seconds_bucket[5m])) by (le)",
          "legendFormat": "{{le}}",
          "format": "heatmap",
          "refId": "A"
        }
      ],
      "options": {
        "calculate": false,
        "yAxis": { "unit": "s" },
        "color": {
          "mode": "scheme",
          "scheme": "Oranges",
          "steps": 128
        },
        "cellGap": 1,
        "tooltip": { "show": true, "yHistogram": true },
        "legend": { "show": true }
      }
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 26 },
      "id": 103,
      "title": "LLM Cost Analytics",
      "type": "row"
    },
    {
      "id": 30,
      "title": "LLM Cost per Hour by Model",
      "description": "Hourly LLM spend broken down by model label. Rate is computed over a 5-minute window and extrapolated to an hourly value.",
      "type": "timeseries",
      "gridPos": { "h": 9, "w": 12, "x": 0, "y": 27 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "sum(rate(aca_llm_cost_total[5m])) by (model) * 3600",
          "legendFormat": "{{model}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "currencyUSD",
          "decimals": 2,
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 20,
            "lineWidth": 2,
            "pointSize": 5,
            "showPoints": "auto",
            "spanNulls": false,
            "stacking": { "mode": "normal" },
            "thresholdsStyle": { "mode": "dashed" }
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "transparent", "value": null },
              { "color": "red", "value": 10 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "tooltip": { "mode": "multi", "sort": "desc" },
        "legend": { "displayMode": "table", "placement": "right", "calcs": ["mean", "max", "lastNotNull"] }
      }
    },
    {
      "id": 31,
      "title": "LLM Cost by Provider",
      "description": "Hourly LLM cost aggregated by provider label.",
      "type": "piechart",
      "gridPos": { "h": 9, "w": 6, "x": 12, "y": 27 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "sum(increase(aca_llm_cost_total[1h])) by (provider)",
          "legendFormat": "{{provider}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "currencyUSD",
          "decimals": 2
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "tooltip": { "mode": "single" },
        "legend": { "displayMode": "table", "placement": "right", "values": ["value", "percent"] },
        "pieType": "donut"
      }
    },
    {
      "id": 32,
      "title": "LLM Token Throughput",
      "description": "Rate of input and output tokens processed per minute across all LLM calls.",
      "type": "timeseries",
      "gridPos": { "h": 9, "w": 6, "x": 18, "y": 27 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "sum(rate(aca_llm_input_tokens_total[5m])) * 60",
          "legendFormat": "Input tokens/min",
          "refId": "A"
        },
        {
          "expr": "sum(rate(aca_llm_output_tokens_total[5m])) * 60",
          "legendFormat": "Output tokens/min",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 15,
            "lineWidth": 2,
            "pointSize": 5,
            "showPoints": "auto",
            "spanNulls": false,
            "stacking": { "mode": "none" }
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "Input tokens/min" },
            "properties": [
              { "id": "color", "value": { "mode": "fixed", "fixedColor": "blue" } }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "Output tokens/min" },
            "properties": [
              { "id": "color", "value": { "mode": "fixed", "fixedColor": "purple" } }
            ]
          }
        ]
      },
      "options": {
        "tooltip": { "mode": "multi", "sort": "desc" },
        "legend": { "displayMode": "list", "placement": "bottom", "calcs": ["mean", "max"] }
      }
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 36 },
      "id": 104,
      "title": "Error Rate & Reliability",
      "type": "row"
    },
    {
      "id": 40,
      "title": "Error Rate Over Time",
      "description": "Rate of errors per second, broken down by error type label. A threshold line marks the alert boundary (10 errors/min = ~0.17/s).",
      "type": "timeseries",
      "gridPos": { "h": 9, "w": 12, "x": 0, "y": 37 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "sum(rate(aca_errors_total[5m])) by (type)",
          "legendFormat": "{{type}}",
          "refId": "A"
        },
        {
          "expr": "sum(rate(aca_errors_total[5m]))",
          "legendFormat": "Total",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "decimals": 3,
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 15,
            "lineWidth": 2,
            "pointSize": 5,
            "showPoints": "auto",
            "spanNulls": false,
            "stacking": { "mode": "none" },
            "thresholdsStyle": { "mode": "dashed" }
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "transparent", "value": null },
              { "color": "red", "value": 0.17 }
            ]
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "Total" },
            "properties": [
              { "id": "color", "value": { "mode": "fixed", "fixedColor": "red" } },
              { "id": "custom.lineWidth", "value": 3 }
            ]
          }
        ]
      },
      "options": {
        "tooltip": { "mode": "multi", "sort": "desc" },
        "legend": { "displayMode": "table", "placement": "right", "calcs": ["mean", "max", "lastNotNull"] }
      }
    },
    {
      "id": 41,
      "title": "Error Budget Remaining (30d)",
      "description": "Remaining error budget for a 99.5% SLO over a 30-day window. A value of 100% means zero errors consumed; 0% means the error budget is exhausted.",
      "type": "gauge",
      "gridPos": { "h": 9, "w": 6, "x": 12, "y": 37 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "clamp_min(\n  1 - (\n    (\n      sum(increase(aca_task_failed_total[30d]))\n      / sum(increase(aca_task_completed_total[30d]) + increase(aca_task_failed_total[30d]))\n    ) / 0.005\n  ),\n0)",
          "legendFormat": "Budget Remaining",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "decimals": 1,
          "min": 0,
          "max": 1,
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "#EAB839", "value": 0.25 },
              { "color": "green", "value": 0.50 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "showThresholdLabels": false,
        "showThresholdMarkers": true
      }
    },
    {
      "id": 42,
      "title": "Errors by Type (1h)",
      "description": "Distribution of error types in the last hour.",
      "type": "piechart",
      "gridPos": { "h": 9, "w": 6, "x": 18, "y": 37 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "sum(increase(aca_errors_total[1h])) by (type)",
          "legendFormat": "{{type}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        },
