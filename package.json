{
  "name": "autonomous-coding-agents",
  "version": "0.1.0",
  "description": "24/7 Autonomous Software Development with Multi-Agent AI System",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "multi-agent": "./dist/cli/index.js"
  },
  "scripts": {
    "dev": "tsx watch src/cli/index.ts",
    "build": "tsc && chmod +x dist/cli/index.js",
    "start": "node dist/cli/index.js",
    "cli": "tsx src/cli/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "ai",
    "automation",
    "coding-agent",
    "multi-agent",
    "llm",
    "github",
    "code-review",
    "autonomous-development"
  ],
  "author": "Kevin",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.74.0",
    "@google/generative-ai": "^0.24.1",
    "@octokit/rest": "^22.0.1",
    "chalk": "^5.6.2",
    "commander": "^12.1.0",
    "dotenv": "^17.2.4",
    "js-yaml": "^4.1.1",
    "octokit": "^4.0.2",
    "openai": "^4.75.0",
    "winston": "^3.19.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.14",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.15.0",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "prettier": "^3.8.1",
    "ts-jest": "^29.4.6",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  }
}
