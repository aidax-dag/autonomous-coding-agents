{
  "name": "autonomous-coding-agents",
  "version": "0.1.0",
  "description": "24/7 Autonomous Software Development with Multi-Agent AI System",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "multi-agent": "./dist/cli/index.js"
  },
  "scripts": {
    "dev": "tsx watch src/cli/index.ts",
    "build": "tsc && chmod +x dist/cli/index.js",
    "start": "node dist/cli/index.js",
    "cli": "tsx src/cli/index.ts",
    "start:agents": "pm2 start ecosystem.config.js",
    "stop:agents": "pm2 stop ecosystem.config.js",
    "restart:agents": "pm2 restart ecosystem.config.js",
    "delete:agents": "pm2 delete ecosystem.config.js",
    "logs": "pm2 logs",
    "logs:coder": "pm2 logs coder-agent",
    "logs:reviewer": "pm2 logs reviewer-agent",
    "logs:repo-manager": "pm2 logs repo-manager-agent",
    "monit": "pm2 monit",
    "dev:coder": "tsx src/bin/start-coder.ts",
    "dev:reviewer": "tsx src/bin/start-reviewer.ts",
    "dev:repo-manager": "tsx src/bin/start-repo-manager.ts",
    "dev:health": "tsx src/bin/start-health-server.ts",
    "dev:api": "tsx watch src/bin/start-api-server.ts",
    "start:api": "node dist/bin/start-api-server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:api": "playwright test --project=api",
    "start:test": "tsx scripts/start-test-server.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "type-check": "tsc --noEmit",
    "db:migrate": "prisma migrate deploy",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts"
  },
  "keywords": [
    "ai",
    "automation",
    "coding-agent",
    "multi-agent",
    "llm",
    "github",
    "code-review",
    "autonomous-development"
  ],
  "author": "Kevin",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.30.0",
    "@fastify/cors": "^11.2.0",
    "@fastify/helmet": "^13.0.2",
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/swagger": "^9.6.1",
    "@fastify/swagger-ui": "^5.2.4",
    "@google/generative-ai": "^0.21.0",
    "@graphql-tools/schema": "^10.0.31",
    "@octokit/rest": "^22.0.1",
    "@prisma/client": "^6.1.0",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "dotenv": "^16.4.5",
    "fastify": "^5.6.2",
    "fastify-plugin": "^5.1.0",
    "glob": "^11.1.0",
    "graphql": "^16.12.0",
    "graphql-scalars": "^1.25.0",
    "js-yaml": "^4.1.1",
    "jsonwebtoken": "^9.0.3",
    "mercurius": "^16.6.0",
    "nats": "^2.29.1",
    "octokit": "^4.0.2",
    "openai": "^4.75.0",
    "ora": "^8.1.1",
    "simple-git": "^3.30.0",
    "uuid": "^13.0.0",
    "winston": "^3.17.0",
    "ws": "^8.19.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@jest/globals": "^29.7.0",
    "@playwright/test": "^1.57.0",
    "@types/jest": "^29.5.14",
    "@types/js-yaml": "^4.0.9",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.17.10",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "pino-pretty": "^13.1.3",
    "prettier": "^3.4.2",
    "prisma": "^7.3.0",
    "ts-jest": "^29.2.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.51.0"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  }
}
